#!/bin/bash

sudo docker build -t rio-vnc -f Dockerfile .

sudo docker tag rio-vnc <%= @basic[:registry_url]+"/rioosvnc:"+@basic[:version] %>

sudo docker login -u <%= @basic[:registry_username] %> -p  <%= @basic[:registry_password] %>  <%= @basic[:registry_url] %>

sudo docker push <%= @basic[:registry_url]+"/rioosvnc:"+@basic[:version] %>
