[Unit]
Description=<%= @package[:package] %> Agent
After=network.target

[Service]
ExecStart=<%= @basic[:home] %>/<%= @package[:package] %>/bin/rioos-gulpd --api-server=http://localhost:9636 --leader-elect=false --rioos-api-content-type=application/json --rioconfig /var/lib/rioos/gulp/gulpd.conf
KillMode=process

[Install]
WantedBy=multi-user.target
